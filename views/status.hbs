<html>
    <head>
         <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
         <link rel="stylesheet" href="/css/menustyle.css">
    </head>

    <body>
        <div id="nav-holder">
            {{> topnavbaruser}}
        </div>
        
        <h1 id="status-header">PLACED ORDERS</h1><br>
        <div id="order-status">
        {{#each data}}
            {{> order
                orderno=orderno
                quantity=quantity
                itemname=itemname
                itemsize=itemsize 
                orderdate=orderdate
                firstname=firstname
                lastname=lastname
                amountdue=amountdue
                status=status
            }}
        {{/each}}
        </div>
    </body>
    
</html>